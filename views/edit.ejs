					</div>
					<div class="field">
						<input type="checkbox" name="vacc[week6.v5.check]" value="True" />	
					</div>
  				</div>
			</div>

			<div class="ui segments">
				<div class="ui segment">
				  <p class="header">10 weeks</p>
				</div>
				<div class="ui horizontal segments">
